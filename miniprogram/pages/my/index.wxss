.mytarBar{
    position: fixed;
    bottom: 0;
    width: 100%;
}
page{
    background-color: #fff;
    padding-bottom: 150rpx;
}
.UCenter-bg {
    background-image: url(https://6865-hello-3ff0d2-1258595957.tcb.qcloud.la/bgImg.jpg?sign=55d11104a877e4ca508c64d9b6e2fd7b&t=1612588249);
    background-size: cover;
    height: 550rpx;
    display: flex;
    justify-content: center;
    padding-top: 40rpx;
    overflow: hidden;
    position: relative;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
  }
  
  .UCenter-bg text {
    opacity: 0.8;
  }
  
  .UCenter-bg image {
    width: 200rpx;
    height: 200rpx;
  }
  
  .UCenter-bg .gif-wave{
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;  
    height: 100rpx;   
  }
  
  map,.mapBox{
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;  
    height: 100rpx;   
  }
  
  map,.mapBox{
    width: 750rpx;
    height: 300rpx;
  }